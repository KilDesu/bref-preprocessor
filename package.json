{
	"name": "brefer",
	"private": true,
	"description": "Svelte 5 preprocessor to shorten runes syntax.",
	"author": "KilDesu",
	"license": "MIT",
	"version": "2.0.0-alpha.1.0",
	"type": "module",
	"engines": {
		"node": "^18.0.0 || >=20",
		"pnpm": "^8.6.3"
	},
	"module": "src/index.js",
