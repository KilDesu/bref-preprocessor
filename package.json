	"repository": {
		"type": "git",
		"url": "git+https://github.com/KilDesu/brefer.git"
	},
	"bugs": {
		"url": "https://github.com/KilDesu/brefer/issues"
	},
	"homepage": "https://github.com/KilDesu/brefer#readme",
	"keywords": [
		"brefer",
		"preprocessor",
		"svelte",
		"svelte 5",
		"runes",
		"vite",
		"vite-plugin-svelte"
	],
	"devDependencies": {
		"@types/node": "^20.10.6",
		"eslint": "^8.56.0",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3",
		"vitest": "^1.1.1",
		"svelte": "5.0.0-next.26"
	},
	"scripts": {
		"test": "vitest",
		"generate:types": "pnpm --filter \"./packages/*\" --parallel generate:types"
