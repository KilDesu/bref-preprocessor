{
	"name": "brefer",
	"description": "Svelte 5 preprocessor to shorten runes syntax.",
	"author": "KilDesu",
	"license": "MIT",
	"version": "2.0.0-alpha.6",
	"type": "module",
	"engines": {
		"node": "^18.0.0 || >=20",
		"pnpm": "^8.6.3"
	},
	"module": "src/index.js",
	"main": "src/index.js",
	"types": "types/index.d.ts",
	"files": [
		"src",
		"types"
	],
	"exports": {
		".": {
			"default": "./src/index.js",
			"types": "./types.d.ts"
		},
		"./package.json": "./package.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KilDesu/brefer.git"
	},
	"bugs": {
		"url": "https://github.com/KilDesu/brefer/issues"
	},
	"homepage": "https://github.com/KilDesu/brefer#readme",
	"keywords": [
		"brefer",
		"preprocessor",
		"svelte",
		"svelte 5",
		"runes",
		"vite",
		"vite-plugin-svelte"
	],
	"dependencies": {
		"@babel/parser": "^7.23.9",
		"recast": "^0.23.4"
	},
	"devDependencies": {
		"@types/node": "^20.11.17",
		"eslint": "^8.56.0",
		"prettier": "^3.2.5",
		"svelte": "5.0.0-next.52",
		"typescript": "^5.3.3",
		"vitest": "^1.2.2",
		"vite": "^5.1.1"
	},
	"scripts": {
		"test": "vitest",
		"generate:types": "dts-buddy -m \"brefer:src/public.d.ts\"",
		"generate:tests": "node ./scripts/generate-module-tests.js"
	}
}
